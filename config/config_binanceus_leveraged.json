      "BNBDOWN/USDT",
      "BTCDOWN/USDT",
      "DOTDOWN/USDT",
      "ETHDOWN/USDT",
      "LINKDOWN/USDT",
      "TRXDOWN/USDT",
      "XRPDOWN/USDT"
